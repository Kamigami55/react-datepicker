    "stylelint-config-recommended",
    "stylelint-config-standard",
    "stylelint-prettier/recommended",
    "stylelint-config-prettier"
  ],
  "plugins": ["stylelint-prettier"],
  "rules": {
    "prettier/prettier": [
      true,
      {
        "singleQuote": false
      }
    ]
  }
}
